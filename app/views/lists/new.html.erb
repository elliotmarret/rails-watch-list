
<h1>Create a New List</h1>

<%= simple_form_for @list do |form| %>
  <div class="form-input">
    <%= form.input :name %>
    <%= form.input :photo, as: :file %>
  </div>
  <br>
  <%= form.submit 'Create List', class: 'btn btn-primary' %>
<% end %>
<br>
<%= link_to 'Return to Lists', lists_path, class: 'btn btn-primary' %>
